datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model ContactSubmission {
  id               String   @id @default(cuid())
  firstName        String
  lastName         String
  email           String
  phone           String
  preferredContact String
  createdAt       DateTime @default(now())
  emailSent       Boolean  @default(false)
}